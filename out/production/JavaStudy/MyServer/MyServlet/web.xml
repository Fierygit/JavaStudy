<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>LoginWeb</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>apply</servlet-name>
		<servlet-class>ApplyWeb</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>apply1</servlet-name>
		<servlet-class>Web1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>apply2</servlet-name>
		<servlet-class>Web2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginWebcss</servlet-name>
		<servlet-class>LoginWebcss</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>firefly</servlet-name>
		<servlet-class>firefly</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>fireflycss</servlet-name>
		<servlet-class>fireflycss</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>firefly</servlet-name>
		<url-pattern>/firefly</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>fireflycss</servlet-name>
		<url-pattern>/fireflycss</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>apply</servlet-name>
		<url-pattern>/app</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>apply1</servlet-name>
		<url-pattern>/web1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>apply2</servlet-name>
		<url-pattern>/web2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginWebcss</servlet-name>
		<url-pattern>/LoginWebcss</url-pattern>
	</servlet-mapping>

</web-app>